{
  "root": "./dist",
  "encoding": "US-ASCII",
  "clean_urls": true,
  "routes": {
    "/assets/*": "/assets/",
    "/**": "index.html"
  },
  "headers": {
    "/**": {
      "Access-Control-Allow-Origin": "*"
    }
  },
  "basic_auth": true,
  "https_only": true,
  "proxies": {
    "/api/": {
      "origin": "https://c-bookstore.herokuapp.com/api/"
    }
  }
}
